{
  "expo": {
    "name": "NeuroLens",
    "slug": "NeuroLens",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "brainlabs.neurolens",
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "url": "https://app.neurolens.me/updates"
    },
    "extra": {
      "googleAuthRedirect": "com.brainlabs.neurolens://"
    },
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "backgroundColor": "#05051E",
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain"
      }
    },
    "android": {
      "package": "com.brainlabs.neurolens",
      "splash": {
        "backgroundColor": "#05051E",
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain"
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#05051E"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["@config-plugins/react-native-webrtc"],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      ["expo-splash-screen"],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/poppins/Poppins_400Regular.ttf"
          ]
        }
      ],
      "expo-secure-store",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
